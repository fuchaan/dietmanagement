<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @menu, local: true) do |form| %>
      <%= form.text_field :name, placeholder: "Nickname" %>
      <%= form.file_field :image, id:"item-image" %>
      <%= form.text_field :calorie, placeholder: "cal" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>